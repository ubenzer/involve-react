{
  "name": "involve-react",
  "version": "0.0.1",
  "description": "A chat application based on firebase",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "compile:prod": "better-npm-run compile:prod",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "compile:prod": {
      "command": "node bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ubenzer/involve-react.git"
  },
  "author": "Umut Benzer <umut@benzer.me>",
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "better-npm-run": "0.0.14",
    "css-loader": "^0.26.2",
    "cssnano": "^3.8.0",
    "debug": "^2.3.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "firebase": "^3.6.0",
    "fs-extra": "^2.0.0",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.7.1",
    "ip": "^1.1.4",
    "json-loader": "^0.5.4",
    "material-ui": "^0.17.0",
    "node-sass": "^4.5.0",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.1.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-form": "^6.2.0",
    "redux-react-firebase": "^2.3.3",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "yargs": "^7.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "babel-plugin-istanbul": "^4.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.0",
    "cheerio": "^0.22.0",
    "codecov": "^1.0.1",
    "connect-history-api-fallback": "^1.3.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.10.0",
    "eslint-config-standard": "^7.0.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-promise": "^3.3.2",
    "eslint-plugin-react": "^6.6.0",
    "eslint-plugin-standard": "^2.0.1",
    "express": "^4.14.0",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack-with-fast-source-maps": "^1.10.0",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "phantomjs-prebuilt": "^2.1.13",
    "react-addons-test-utils": "^15.3.2",
    "redbox-react": "^1.3.3",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  }
}
